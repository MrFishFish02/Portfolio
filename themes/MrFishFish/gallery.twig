{% include 'incl/header.twig' %}
<body>
    {% include 'incl/nav.twig' %}
    <div class="main" role="main">
        <div class="grid-p">
            {{ content }}
            {% for image in pages["_meta"].meta.gallery %}
                <a href="{{ base_url }}{{ image.url }}" target="_blank">
                    <picture>
                    <img src="{{ base_url }}{{ image.url }}?w=320&h=250&crop-to-fit" alt="{{ image.alt }}"/>
                    </picture>
                </a>
            {% endfor %}
        </div>
        <div class="embed-container">
        <iframe src="https://www.youtube.com/embed/gtnehMIdLMM" frameborder="0" allowfullscreen></iframe>
        </div>
    </div>
    {% include 'incl/footer.twig' %}
</body>

</html>
